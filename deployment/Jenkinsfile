pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh '''
			deployment/build/mvn.sh mvn -B -DskipTests clean package
			deployment/build/build.sh
                    '''   
            }
            post {
                success {
                    archiveArtifacts artifacts: './target/*.jar', fingerprint: true
                }
            }
        }
    }
}
